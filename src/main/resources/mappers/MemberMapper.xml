<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chicken.project.member.model.dao.MemberMapper">

    <resultMap id="employeeResultMap" type="EmployeeDTO">
        <id property="empId" column="EMP_ID"/>
        <result property="empName" column="EMP_NAME"/>
        <result property="empPhone" column="EMP_PHONE"/>
        <result property="empEmail" column="EMP_EMAIL"/>
        <result property="managerYn" column="MANAGER_YN"/>
        <result property="empPwd" column="EMP_PWD"/>
        <result property="birth" column="EMP_BIRTH"/>
        <result property="memberType" column="MEMBER_TYPE"/>

        <association property="store" resultMap="storeResultMap"/>
    </resultMap>

    <resultMap id="storeResultMap" type="StoreDTO">
        <id property="storeName" column="STORE_NAME"/>
        <id property="storeAccount" column="STORE_ACCOUNT"/>
        <result property="storeRep" column="STORE_REP"/>
        <result property="storeAddress" column="STORE_ADDRESS"/>
        <result property="storePhone" column="STORE_PHONE"/>
        <result property="storeEmail" column="STORE_EMAIL"/>
        <result property="storeId" column="STORE_ID"/>
        <result property="storePwd" column="STORE_PWD"/>
        <result property="storeNo" column="STORE_NO"/>
        <result property="memberType" column="MEMBER_TYPE"/>
    </resultMap>


    <select id="selectMember" resultMap="employeeResultMap">

    </select>
</mapper>
